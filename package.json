    "@ctrl/ngx-emoji-mart": "^6.0.0",
    "@fluentui/react": "^7.166.2",
    "@fluentui/react-icons-northstar": "^0.51.7",
    "@fluentui/react-northstar": "^0.51.7",
    "@types/preval.macro": "^3.0.0",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "@webscopeio/react-textarea-autocomplete": "^4.7.3",
    "bootstrap": "^4.6.0",
    "emoji-datasource-apple": "^7.0.2",
    "emoji-datasource-facebook": "^7.0.2",
    "emoji-datasource-google": "^7.0.2",
    "emoji-datasource-twitter": "^7.0.2",
